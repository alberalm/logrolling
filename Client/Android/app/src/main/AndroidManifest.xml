<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.logrolling.client">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RealizarFavor" />
        <activity android:name=".Mensajes" />
        <activity android:name=".Favores" />
        <activity android:name=".Tienda" />
        <activity android:name=".MiPerfil" />
        <activity android:name=".Configuracion" />
        <activity android:name=".Regalos" />
        <activity android:name=".PedirFavor" />
        <activity android:name=".MisFavores" />
        <activity android:name=".Registro" />
        <activity android:name=".InicioSesion" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>